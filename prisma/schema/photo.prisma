model Photo {
  id_photo            Int                 @id @default(autoincrement())
  path                String              @db.VarChar(255)
  alt                 String?       @db.VarChar(255)
  id_fab_ordinateur   Int?
  id_fab_logiciel     Int?
  id_logiciel         Int?
  id_ordinateur       Int?
  id_fab_emulateur    Int?
  id_emulateur        Int?
  id_article          Int?                @unique
  article             Article?             @relation(fields: [id_article], references: [id_article])
  emulateur           Emulateur?           @relation(fields: [id_emulateur], references: [id_emulateur])
  fabricantEmulateur  FabricantEmulateur?  @relation(fields: [id_fab_emulateur], references: [id_fab_emulateur])
  fabricantLogiciel   FabricantLogiciel?   @relation(fields: [id_fab_logiciel], references: [id_fab_logiciel])
  fabricantOrdinateur FabricantOrdinateur? @relation(fields: [id_fab_ordinateur], references: [id_fab_ordinateur])
  logiciel            Logiciel?            @relation(fields: [id_logiciel], references: [id_logiciel])
  ordinateur          Ordinateur?          @relation(fields: [id_ordinateur], references: [id_ordinateur])
  utilisateur         Utilisateur?

  @@index([id_emulateur], map: "Photo_id_emulateur_fkey")
  @@index([id_fab_emulateur], map: "Photo_id_fab_emulateur_fkey")
  @@index([id_fab_logiciel], map: "Photo_id_fab_logiciel_fkey")
  @@index([id_fab_ordinateur], map: "Photo_id_fab_ordinateur_fkey")
  @@index([id_logiciel], map: "Photo_id_logiciel_fkey")
  @@index([id_ordinateur], map: "Photo_id_ordinateur_fkey")
}
