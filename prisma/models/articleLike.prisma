model ArticleLike {
  id_article_like Int         @id @default(autoincrement())
  score           Int
  id_article      Int
  id_utilisateur  Int
  article         Article     @relation(fields: [id_article], references: [id_article])
  utilisateur     Utilisateur @relation(fields: [id_utilisateur], references: [id_utilisateur])

  @@index([id_article], map: "ArticleLike_id_article_fkey")
  @@index([id_utilisateur], map: "ArticleLike_id_utilisateur_fkey")
}
