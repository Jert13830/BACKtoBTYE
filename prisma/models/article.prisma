model Article{
    id_article Int @id @default(autoincrement())

    titre   String @db.VarChar(255)
    texte   String
    date    DateTime

    categorie  Categorie   @relation(fields: [id_categorie], references: [id_categorie])
    id_categorie Int
   
    ordinateur  Ordinateur   @relation(fields: [id_ordinateur], references: [id_ordinateur])
    id_ordinateur   Int

    utilisateur  Utilisateur   @relation(fields: [id_utilisateur], references: [id_utilisateur])
    id_utilisateur Int

    commentaires    Commentaire[]
    articleLikes    ArticleLike[]
    photo Photo?
}